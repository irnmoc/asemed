<script type="text/template" id="tmpl-elementor-panel-revisions-no-revisions">
	<#
	var no_revisions_1 = 'El histórico de revisiones te permitir guardar las versiones previas de tu trabajo, y recuperarlas cuando quieras.',
		no_revisions_2 = 'Empieza a diseñar tu página y serás capaz de ver aquí el historial completo de revisiones.',
		revisions_disabled_1 = 'Parece que no está disponible en tu sitio la funcionalidad de revisión de entradas.',
		revisions_disabled_2 = 'Aprende más sobre las <a target="_blank" href="https://go.elementor.com/wordpress-revisions/">revisiones de WordPress</a>';
	#>
	<img class="elementor-nerd-box-icon" src="https://www.asemed.org/wp-content/plugins/elementor/assets/images/information.svg" loading="lazy" alt="Elementor" />
	<div class="elementor-nerd-box-title">Todavía no hay revisiones guardadas</div>
	<div class="elementor-nerd-box-message">{{{ elementor.config.document.revisions.enabled ? no_revisions_1 : revisions_disabled_1 }}}</div>
	<div class="elementor-nerd-box-message">{{{ elementor.config.document.revisions.enabled ? no_revisions_2 : revisions_disabled_2 }}}</div>
</script>

